{
	"manifest_version": 3,
	"name": "Test extension",
	"description": "Save your browser tabs",
	"version": "0.0.1",
	"permissions": [
		"tabs",
		"cookies"
	],
	"host_permissions": [
		"http://127.0.0.1:5173/"
	],
	"action": {
		"default_popup": "index.html"
	},
	"background": {
		"service_worker": "background_script.js"
	},
	"content_scripts": [
		{
			"js": [
				"content_script.js"
			],
			"matches": [
				"http://127.0.0.1:5173/*"
			]
		}
	]
}